{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "dependencyDashboard": false,
  "regexManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^Jenkinsfile$"],
      "matchStrings": [
          "# renovate: datasource=(?<datasource>.*?) depName=(?<depName>.*?)\nv(?<currentValue>.*)"
      ]
    }
  ]
}